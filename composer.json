{
    "require": {
        "silverstripe/installer": "^4.7",
        "lekoala/silverstripe-uuid": "dev-master"
    },
    "extra": {
        "project-files-installed": [
            ".env.example",
            "app/.htaccess",
            "app/_config.php",
            "app/_config/assets.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/_config/theme.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "assets/.gitignore",
            "assets/.htaccess",
            "assets/web.config",
            "favicon.ico",
            "index.php",
            "web.config"
        ]
    }
}
